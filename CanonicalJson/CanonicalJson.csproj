<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>Stratumn.CanonicalJson</PackageId>
    <Title>Stratumn CanonicalJson</Title>
    <Description>C# library for producing JSON in canonical format</Description>
    <Version>0.0.8.0</Version>
    <Authors>Stratumn</Authors>
    <Company>Stratumn</Company>
    <PackageIconUrl>https://avatars2.githubusercontent.com/u/15095987</PackageIconUrl>
    <PackageLicenseUrl>https://github.com/stratumn/canonicaljson-csharp/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/stratumn/canonicaljson-csharp</RepositoryUrl>
    <PackageTags>json;canonical</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="dmath.core" Version="1.7.0" />
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

</Project>
